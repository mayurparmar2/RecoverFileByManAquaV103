<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
                android:centerColor="#ff7e6c"
                android:endColor="#ff7e6c"
                android:startColor="#ff7e6c"
                android:type="sweep" />
            <corners android:radius="@dimen/_10sdp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#0d1618" />
            <corners android:radius="@dimen/_10sdp" />
        </shape>
    </item>
</layer-list>
